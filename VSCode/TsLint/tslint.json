{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
    	"indent": [true, "tabs", 4],
    	"no-console": [false, "log", "error"],
    	"ordered-imports": false,
    	"variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"],
        "no-trailing-whitespace": false,
        "object-literal-sort-keys": false,
    },
    "rulesDirectory": []
}